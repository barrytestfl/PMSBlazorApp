                <input id="radioArrivals" name="ListViewType" type="radio" value="1" />
                <label for="radioArrivals" class="btn btn-grouped btn-invert btn-icon btn-respond">
                    <svg class="icon-bg icon-bg-room_service"><use xlink:href="#icon-bg-room_service"></use></svg>
                    <span>Arrivals</span>
                </label>
                <input id="radioInHouse" name="ListViewType" type="radio" value="2" />
                <label for="radioInHouse" class="btn btn-grouped btn-invert btn-icon btn-respond">
                    <svg class="icon-bg icon-bg-local_hotel"><use xlink:href="#icon-bg-local_hotel"></use></svg>
                    <span>In House</span>
                </label>
                <input id="radioDepatures" name="ListViewType" type="radio" value="3" />
                <label for="radioDepatures" class="btn btn-grouped btn-invert btn-icon btn-respond">
                    <svg class="icon-bg icon-bg-work-empty"><use xlink:href="#icon-bg-work-empty"></use></svg>
                    <span>Departures</span>
                </label>
            </div>
        </div>
        <div class="s-list-views mod-toolbar_section mod-toolbar_section buttons-light" data-order="4">
            <div class="cp-radio-group radio-group-small cp-radio-group_single float-left mr10">
                <input id="radioNew" name="ListViewType" type="radio" value="4" />
                <label for="radioNew" class="btn btn-invert">
                    <span>Just In</span>
                </label>
            </div>
            <div class="cp-radio-group radio-group-small cp-radio-group_single float-left mr10">
                <input id="radioBooked" name="ListViewType" type="radio" value="5" />
                <label for="radioBooked" class="btn btn-invert">
                    <span>Booked</span>
                </label>
            </div>
        </div>
        <div class="s-list-views mod-toolbar_section mod-toolbar_section__no-right-border buttons-light" data-order="5">
            <div class="cp-custom-input">
                <input type="checkbox" id="withInvoice" />
                <label for="withInvoice">
                    <span class="checkbox-input"></span>
                    <span class="label">With Invoice</span>
                </label>
            </div>
        </div>

        <div class="mod-toolbar_section mod-toolbar_section__end" data-order="5">
            <div class="toolbar-select">
                <button id="btnActions" disabled class="btn btn-icon btn-icon_after mr10">
                    <span>Bulk actions</span>
                    <svg class="icon-bg icon-bg-expand_more"><use xlink:href="#icon-bg-expand_more"></use></svg>
                </button>
                <div class="dropdown-outer align-right">
